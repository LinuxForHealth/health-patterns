{
    "scenarioName": "********enrich_NLP_Insights_Config_Use********",
    "ignoreStepFailures": false,
    "steps": [ 

        {
            "name": "createConfig${0}-${1}",
            "url": "${nlp_insights}/config/test-${1}",
            "operation": "${0}",           
                "request": {
                "body" : { "nlpService" : "${1}"} 
                },            
            "verify": {
                "status": 200
            }
        },
        {
            "name": "activateConfig${0}-${1}",
            "url": "${nlp_insights}/config?name=test-${1}",
            "operation": "${0}",           
                "request": { },            
            "verify": {
                "status": 200
            }
        },        
        {
            "name": "getActiveConfig${0}-${1}",
            "url": "${nlp_insights}/config",
            "operation": "GET",           
                "request": { },            
            "verify": {
                "status": 200,
                "body": {
                  "nlpService":"$MATCHES.STRING:${1}"}
            }
        },
        {  
            "name": "processAnalytics",
            "url": "${nlp_insights}/process",
            "operation": "POST",           
                "request": {
                    "body":  "${2}" },            
            "verify": {
                "status": 200
            }
        }, 
        {
            "name": "verifyAnalyticType${0}-${1}",
            "url": "utilities.ZerocodeUtilities",
            "method": "containsString",
            "request": {
                 "string1": "${$.processAnalytics.response.body}",
                 "string2": "${3}"
            },
            "verify": { "result": "true" }
        },            
        {
            "name": "activateDefault${0}-${1}",
            "url": "${nlp_insights}/config?name=default",
            "operation": "${0}",           
                "request": { },            
            "verify": {
                "status": 200
            }
        },
        {
            "name": "getDefaultConfig${0}-${1}",
            "url": "${nlp_insights}/config",
            "operation": "GET",           
                "request": { },            
            "verify": {
                "status": 200,
                "body": {
                  "nlpService":"$MATCHES.STRING:quickUMLS"}
            }
        }                 
     ],
     "parameterized": {
        "csvSource":[
            "PUT,ACD,${JSON.FILE:Patients/patientbundleNLP.json},ACD Structured",
            "POST,quickUMLS,${JSON.FILE:Patients/patientbundleNLP.json},QuickUMLS Unstructured",
            "POST,ACD,${JSON.FILE:Patients/diagnosticreportNLP.json},ACD Unstructured",
            "PUT,quickUMLS,${JSON.FILE:Patients/diagnosticreportNLP.json},QuickUMLS Unstructured",
            "PUT,ACD,${JSON.FILE:Patients/allergyintoleranceNLP.json},ACD Structured",
            "POST,quickUMLS,${JSON.FILE:Patients/allergyintoleranceNLP.json},QuickUMLS Structured",
            "POST,ACD,${JSON.FILE:Patients/immunizationNLP.json},ACD Structured",
            "PUT,quickUMLS,${JSON.FILE:Patients/immunizationNLP.json},QuickUMLS Structured"

        ]
    }
}
